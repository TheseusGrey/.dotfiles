{
  "height": 48,
  "margin": "8px 8px 0 8px",
  "spacing": 4,
  "mode": "dock",
  "modules-left": [
    "hyprland/workspaces",
    "hyprland/window"
  ],
  "modules-center": [],
  "modules-right": [
    "clock"
  ],
  "clock": {
    "tooltip": true,
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "{:%Y-%m-%d}"
  }
}
