. "$HOME/.cargo/env"

export DOTFILES=~/.dotfiles
export EDITOR=nvim
export TERMINAL=kitty

export FZF_DEFAULT_OPTS=$FZF_DEFAULT_OPTS"
    --height 62% --layout=reverse \
    --color=fg:#D8DEE9,bg:#242933,hl:#81a1c1
